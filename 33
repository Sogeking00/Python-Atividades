a = int(input('Digite um número: '))
b = int(input('Digite outro número: '))
c = int(input('Digite mais um número: '))
maior = a
if b>a and b>c:
  maior = b
if c>a and c>b:
  maior = c
print(f'O maior número é {maior}')
menor = a
if b<a and b<c:
  maior = b
if c<a and c<b:
  maior = c
print(f'O menor número é {menor}')
